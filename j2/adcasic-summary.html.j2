{% include 'header.html.j2' %}
    

<script type="text/javascript" class="init">
  $(document).ready(function() {
  $('#summary').DataTable();
  } );
  </script>

<h1>ADC ASIC Test Summary for ADC# {{ adcasic.serial }} sampled on {{ adcasic.timestamp }}</h1>


<h2>Links</h2>
<ul>
  <li><a href="{{ reltoroot }}/adcid/{{ adcasic.serial }}/">All results for ADC# {{ adcasic.serial }}</a></li>
  <li><a href="http://localhost:8942/adc/{{ label }}/">Sumatra entry</a> (requires local Sumatra server or SSH tunnel)</li>
</ul>

<h2>Summary information</h2>

<table id="summary" class="display" width="100%" cellspacing="0">
  <thead>
    <tr>
      <th>Parameter</th>
      <th>Value</th>
    </tr>
  </thead>
  <tbody>
    <tr><td>Passed</td><td>{{ adcasic.pass }}</td></tr>
    <tr><td>ADC ASIC sn#</td><td><a href="{{ reltoroot }}/adcid/{{ adcasic.serial }}">{{ adcasic.serial }}</a></td></tr>
    <tr><td>Timestamp</td><td>{{ adcasic.timestamp }}</td></tr>
    <tr><td>Test board</td><td><a href="{{ reltoroot }}/adcboard/{{ adcasic.board_id }}">{{ adcasic.board_id }}</a></td></tr>
    <tr><td>DAQ host</td><td>{{ adcasic.hostname }}</td></tr>
    <tr><td>DAQ version</td><td>{{ adcasic.version }}</td></tr>
    <tr><td>DAQ Config</td><td>{{ adcasic.femb_config }}</td></tr>
  </tbody>
</table>

<h2>Summary Plots</h2>

{% for png in adcasic.png -%}
<h3>{{ png }}</h3>
<img src="{{ png }}">
{% endfor -%}


{% include 'footer.html.j2' %}
    
